class Calculation_at_once{
  public static void main(String[] args){
    int a=10;
    int b=5;
    int c=a+b;
    int d=c-b;
    int e =d*a;
    int f=e/b;
      System.out.println("The value is "+c);
      System.out.println("The value is "+d);
      System.out.println("The value is "+e);
      System.out.println("The value is "+f);
 }
} 
